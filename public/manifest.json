{
  "name": "WTab",
  "description": "WTab标签页的浏览器插件",
  "version": "0.0.1",
  "manifest_version": 3,
  "start_url": "index.html",
  "display": "standalone",
  "icons": [
    {
      "src": "/favicon.png",
      "sizes": "200x200"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "WTab",
    "default_icon": [
      {
        "src": "/favicon.ico",
        "sizes": "192x192",
        "type": "image/png"
      }
    ],
    "default_popup": "index.html"
  }
}
